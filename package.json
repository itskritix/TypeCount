{
  "name": "typecount",
  "productName": "TypeCount",
  "version": "1.0.0",
  "description": "Professional keystroke analytics and productivity tracking application. Monitor your typing patterns, unlock achievements, and optimize your productivity with real-time analytics and gamification.",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "rebuild": "npx @electron/rebuild --version=39.2.3",
    "rebuild:uiohook": "npx @electron/rebuild --version=39.2.3 --module-dir node_modules/uiohook-napi",
    "postinstall": "npx @electron/rebuild --version=39.2.3",
    "generate:installer-gif": "node scripts/generate-installer-gif.js",
    "generate:installer-assets": "node scripts/generate-installer-assets.js",
    "build": "electron-forge package"
  },
  "keywords": [
    "productivity",
    "typing",
    "analytics",
    "keystroke",
    "monitoring",
    "gamification",
    "macOS",
    "electron",
    "desktop-app"
  ],
  "author": {
    "name": "everyrank.app",
    "email": "itskritix@gmail.com"
  },
  "homepage": "https://everyrank.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/itskritix/TypeCount.git"
  },
  "bugs": {
    "url": "https://github.com/typecount/typecount/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-dmg": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/plugin-vite": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^4.0.1",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/fs-extra": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "dotenv": "^17.2.3",
    "electron": "39.2.3",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.32.0",
    "fs-extra": "^11.3.2",
    "png-to-ico": "^3.0.1",
    "sharp": "^0.33.5",
    "typescript": "~4.5.4",
    "vite": "^5.4.21"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.84.0",
    "auto-launch": "^5.0.6",
    "chart.js": "^4.5.1",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^11.0.2",
    "electron-updater": "^6.6.2",
    "uiohook-napi": "^1.5.4"
  }
